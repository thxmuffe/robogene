# Copy this file to robogen.debug.env and fill real secrets/values.
# This file is read by scripts/run-robogene.js.

FUNCTIONS_WORKER_RUNTIME='node'

# Required storage settings
ROBOGENE_STORAGE_CONNECTION_STRING='DefaultEndpointsProtocol=https;AccountName=<storage-account>;AccountKey=<storage-key>;EndpointSuffix=core.windows.net'
AzureWebJobsStorage='DefaultEndpointsProtocol=https;AccountName=<storage-account>;AccountKey=<storage-key>;EndpointSuffix=core.windows.net'

# Required realtime setting
AzureSignalRConnectionString='Endpoint=https://<signalr-name>.service.signalr.net;AccessKey=<signalr-key>;Version=1.0;'

# Required image generation setting
# ROBOGENE_IMAGE_GENERATOR_KEY is the provider secret (OpenAI API key or equivalent).
# Supported ROBOGENE_IMAGE_GENERATOR values: openai, mock
ROBOGENE_IMAGE_GENERATOR='openai'
ROBOGENE_IMAGE_GENERATOR_KEY='<image-generator-api-key>'

# Optional image model tuning
ROBOGENE_IMAGE_MODEL='gpt-image-1-mini'
ROBOGENE_IMAGE_QUALITY='low'
ROBOGENE_IMAGE_SIZE='1024x1024'

# Required CORS list (used by API host and passed to func --cors)
ROBOGENE_ALLOWED_ORIGIN='http://localhost:8080,http://127.0.0.1:8080,http://localhost:5500,http://127.0.0.1:5500,http://localhost:3000,http://127.0.0.1:3000,http://localhost:5173,http://127.0.0.1:5173'
