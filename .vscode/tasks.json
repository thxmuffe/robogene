{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm: start",
      "type": "npm",
      "script": "start",
      "isBackground": true,
      "problemMatcher": [
        {
          "owner": "robogene-start",
          "pattern": [
            {
              "regexp": "a^",
              "kind": "file",
              "file": 0,
              "message": 0
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": ".+",
            "endsPattern": "watching build :webapp-debug"
          }
        }
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "dedicated",
        "clear": false
      }
    },
    {
      "label": "npm: test",
      "type": "npm",
      "script": "test",
      "problemMatcher": [],
      "group": "test",
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      }
    }
  ]
}
