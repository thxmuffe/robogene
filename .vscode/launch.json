{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Robogene: Full Debug",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/run-robogene.js",
      "cwd": "${workspaceFolder}",
      "outputCapture": "std",
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "serverReadyAction": {
        "pattern": "Webapp: http://localhost:([0-9]+)",
        "uriFormat": "http://localhost:%s/index.html",
        "action": "debugWithChrome"
      }
    }
  ]
}
