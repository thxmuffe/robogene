{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Robogene: Services (F5)",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start"
      ],
      "outputCapture": "std",
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "serverReadyAction": {
        "pattern": "shadow-cljs - HTTP server available at (http://localhost:[0-9]+)",
        "action": "startDebugging",
        "name": "Robogene: Open Chrome (reuse window)"
      },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/src/webapp/**",
        "!${workspaceFolder}/src/webapp/js/cljs-runtime/node_modules/**",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Robogene: Open Chrome (reuse window)",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "open",
      "runtimeArgs": [
        "-a",
        "Google Chrome",
        "http://localhost:8080/index.html"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Robogene: Browser Debug (optional)",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:8080/index.html",
      "webRoot": "${workspaceFolder}/src/webapp",
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/src/webapp/**",
        "!${workspaceFolder}/src/webapp/js/cljs-runtime/node_modules/**",
        "!**/node_modules/**"
      ]
    }
  ]
}
