{
  "name": "robogene-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch": "shadow-cljs watch app",
    "backend:watch": "shadow-cljs watch backend",
    "watch:all": "shadow-cljs watch app backend",
    "backend:start": "npm --prefix backend run start",
    "build": "shadow-cljs release app",
    "build:backend": "shadow-cljs release backend",
    "build:all": "shadow-cljs release app backend",
    "test": "node --test tests/*.test.mjs",
    "test:e2e": "ROBOGENE_RUN_E2E=1 node --test tests/e2e-backend-api.test.mjs",
    "test:all": "npm test && npm run test:e2e"
  },
  "dependencies": {
    "@azure/functions": "^4.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "shadow-cljs": "^2.28.20"
  }
}
