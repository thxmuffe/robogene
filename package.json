{
  "name": "robogene",
  "version": "0.1.0",
  "private": true,
  "main": "src/host/dist/main.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "start": "node scripts/run-robogene.js",
    "start:release": "node scripts/run-robogene.js --release",
    "watch": "shadow-cljs watch webapp-debug",
    "webapi:watch": "shadow-cljs watch webapi-debug host-debug",
    "watch:all": "shadow-cljs watch webapp-debug webapi-debug host-debug",
    "host:start": "func start --script-root src/host",
    "build": "shadow-cljs release webapp-release",
    "build:webapi": "shadow-cljs release webapi-release host-release",
    "build:webapi:debug": "shadow-cljs compile webapi-debug host-debug",
    "package:webapi": "node scripts/package-webapi-dist.js",
    "dist:webapi": "npm run build:webapi && npm run package:webapi",
    "build:all": "shadow-cljs release webapp-release webapi-release host-release",
    "test": "node --test tests/webapp/*.test.mjs tests/services/*.test.mjs",
    "test:webapp": "node --test tests/webapp/*.test.mjs",
    "test:services": "node --test tests/services/*.test.mjs",
    "test:e2e": "npm run test:e2e:ui",
    "test:e2e:ui": "ROBOGENE_RUN_E2E_UI=1 node --test tests/end-to-end/e2e-gallery-ui.test.mjs",
    "test:e2e:env": "node scripts/run-with-env.js robogen.test.env -- npm run test:e2e",
    "test:e2e:ui:env": "node scripts/run-with-env.js robogen.test.env -- npm run test:e2e:ui",
    "test:all": "npm test && npm run test:e2e"
  },
  "dependencies": {
    "@azure/data-tables": "^13.3.2",
    "@azure/functions": "^4.11.2",
    "@azure/storage-blob": "^12.31.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@microsoft/signalr": "^10.0.0",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "sweetalert2": "^11.26.20"
  },
  "devDependencies": {
    "playwright": "^1.54.2",
    "shadow-cljs": "^3.3.6"
  }
}
