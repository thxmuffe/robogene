{
  "name": "robogene",
  "version": "0.1.0",
  "private": true,
  "main": "src/hosting/index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "watch": "cd config && shadow-cljs watch app",
    "backend:watch": "cd config && shadow-cljs watch backend",
    "watch:all": "cd config && shadow-cljs watch app backend",
    "backend:start": "func start --script-root src/hosting --cors \"http://localhost:8080,http://127.0.0.1:8080\"",
    "build": "cd config && shadow-cljs release app",
    "build:backend": "cd config && shadow-cljs release backend",
    "build:all": "cd config && shadow-cljs release app backend",
    "test": "node --test tests/*.test.mjs",
    "test:e2e": "ROBOGENE_RUN_E2E=1 node --test tests/e2e-backend-api.test.mjs",
    "test:all": "npm test && npm run test:e2e"
  },
  "dependencies": {
    "@azure/data-tables": "^13.3.2",
    "@azure/functions": "^4.11.2",
    "@azure/storage-blob": "^12.31.0",
    "@microsoft/signalr": "^9.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sweetalert2": "^11.26.20"
  },
  "devDependencies": {
    "shadow-cljs": "^2.28.20"
  }
}
