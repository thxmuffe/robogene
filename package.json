{
  "name": "robogene",
  "version": "0.1.0",
  "private": true,
  "main": "src/api_host/story_routes_host.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "watch": "cd build && shadow-cljs watch webapp",
    "webapi:watch": "cd build && shadow-cljs watch webapi",
    "watch:all": "cd build && shadow-cljs watch webapp webapi",
    "api_host:start": "func start --script-root src/api_host --cors \"http://localhost:8080,http://127.0.0.1:8080\"",
    "build": "cd build && shadow-cljs release webapp",
    "build:webapi": "cd build && shadow-cljs release webapi",
    "package:webapi": "./.github/azure_deploy/package_webapi_dist.sh",
    "dist:webapi": "npm run build:webapi && npm run package:webapi",
    "build:all": "cd build && shadow-cljs release webapp webapi",
    "test": "node --test tests/webapp/*.test.mjs tests/services/*.test.mjs",
    "test:webapp": "node --test tests/webapp/*.test.mjs",
    "test:services": "node --test tests/services/*.test.mjs",
    "test:e2e": "ROBOGENE_RUN_E2E=1 node --test tests/end-to-end/e2e-webapi.test.mjs",
    "test:all": "npm test && npm run test:e2e"
  },
  "dependencies": {
    "@azure/data-tables": "^13.3.2",
    "@azure/functions": "^4.11.2",
    "@azure/storage-blob": "^12.31.0",
    "@microsoft/signalr": "^9.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sweetalert2": "^11.26.20"
  },
  "devDependencies": {
    "shadow-cljs": "^2.28.20"
  }
}
