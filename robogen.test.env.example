# Optional: tune test timeouts and polling behavior.
ROBOGENE_E2E_TIMEOUT_MS='45000'
ROBOGENE_IMAGE_GEN_TIMEOUT_MS='180000'
ROBOGENE_IMAGE_GEN_POLL_MS='2000'
